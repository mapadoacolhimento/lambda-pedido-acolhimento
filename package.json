{
  "name": "lambda-pedido-acolhimento",
  "version": "0.0.1",
  "description": "AWS Lambda com API Gateways que fazem tudo relacionado ao processamento do pedido de acolhimento da MSR.",
  "scripts": {
    "prepare": "husky install",
    "dev": "serverless offline",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc",
    "deploy": "serverless deploy",
    "package": "serverless package",
    "test": "jest --watch",
    "test:stage": "jest --findRelatedTests --passWithNoTests",
    "test:ci": "jest --ci",
    "clean:build": "rimraf .serverless dist",
    "db:start": "docker compose up -d",
    "db:push": "prisma db push",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev",
    "migrate:diff": "prisma migrate diff",
    "generate": "prisma generate"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@tsconfig/node-lts-strictest": "^18.12.1",
    "@types/aws-lambda": "^8.10.126",
    "@types/dotenv-safe": "^8.1.5",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^15.1.0",
    "prettier": "^3.0.3",
    "prisma": "^5.6.0",
    "rimraf": "^5.0.5",
    "serverless": "^3.36.0",
    "serverless-offline": "^13.2.1",
    "serverless-webpack": "^5.13.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "transform": {
      "\\.[jt]sx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      ".serverless"
    ],
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@turf/turf": "^6.5.0",
    "dotenv-safe": "^8.2.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "yup": "^1.3.2"
  }
}
